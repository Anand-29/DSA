6//balancing paranthesis
#include<stdio.h>
#include<string.h>
#define MAX 10
char stack[MAX];
int top=-1;
void push(char val){
    stack[++top]=val;
}
char peek(){
    return stack[top];
}
void pop(){
    top--;
}
int main(){
    printf("Enter the Expression:-\n");
    char s[MAX];
    scanf("%s",s);
    for(int i=0;i<strlen(s);i++){
        if(s[i]=='{' || s[i]=='(' || s[i]=='[')
            push(s[i]);
        else{
            char v=peek();
            switch(v)
                pop();
            else{
                top=MAX;
                break;
            }
        }
    }
    if(top==-1)
        printf("Balanced");
    else
        printf("Not Balanced");
    return 0;
}
